                        <% var places = places %>
                        <% for (index in places){%>
                            <div id="card-group">
                                <% var varId = places[index]['_id'].toString()%>
                                <form id=<%= varId%> name="save_recent" action = "/save_recent" method = "POST" target = "dummyFrame">
                                    <div class="card-image">
                                        <img draggable="false" src=<%= places[index]['Images Attached']%>>
                                    </div>
                                    <div class="card-body">
                                        <h2 class="font-black" id="title"><%= places[index]['Title']%></h2>
                                        <p class="font-black" id="content"><%= places[index]['Description']%></p>
                                        <% var link = places[index]['Link']%>
                                        <div id="link-buttons">
                                            <input type = "hidden" name = "varId" id = "varId" value=<%= varId%>>
                                            <input type = "hidden" name = "Category" id = "Category" value ='Places'>
                                            <a data-href="/find-place?lat=<%= places[index]['Lat/Long']['0']%>&lng=<%= places[index]['Lat/Long']['1']%>" onclick="return saveRecent('<%- varId%>')" target="_blank" class="buttoncard">Check Place</a>
                                        </div>
                                    </div>
                                </form>
                            </div>
                            <script type="text/javascript">
                                function saveRecent(id){
                                    document.getElementById(id).submit();
                                    return true;
                                }
                            </script>
                            <iframe name = "dummyFrame" style="display: none;"></iframe>
                        <% } %>